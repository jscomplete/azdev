{
  "name": "az.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:tsc": "yarn tsc --noEmit -p .",
    "dev:eslint": "eslint \"**/src/**/*.{ts,tsx}\"",
    "dev:schema-lint": "graphql-schema-linter src/schema/schema.graphql",
    "dev:format": "prettier --write .",
    "dev:db-server": "docker-compose -f dev-db/docker.yml up",
    "dev:web-server": "cross-env NODE_PATH=./src tsnd --respawn src/server/server.ts --ignore-watch public",
    "dev:web-bundler": "webpack --mode=development --progress -w --env=dev"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "apollo-server": "^2.21.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.0.6",
    "css-loader": "^5.1.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "graphql": "^15.5.0",
    "graphql-schema-linter": "^2.0.1",
    "helmet": "^4.4.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.2.0",
    "lzutf8": "^0.6.0",
    "mini-css-extract-plugin": "^1.3.9",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pm2": "^4.5.5",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "serialize-javascript": "^5.0.1",
    "tailwindcss": "^2.0.3",
    "ts-invariant": "^0.6.1",
    "ts-loader": "^8.0.17",
    "ts-node-dev": "^1.1.6",
    "ts-polyfill": "^3.8.2",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^4.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-session": "^2.0.42",
    "@types/copyfiles": "^2.4.0",
    "@types/cors": "^2.8.10",
    "@types/ejs": "^3.0.6",
    "@types/eslint": "7.2.6",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/morgan": "^1.9.2",
    "@types/pg": "^7.14.10",
    "@types/postcss-import": "^12.0.0",
    "@types/prettier": "^2.2.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/serialize-javascript": "^5.0.0",
    "@types/webpack": "^4.41.26",
    "@types/webpack-chunk-hash": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "7.21.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "typesync": "^0.8.0"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": [
    "> 0.5%, last 2 versions, not dead"
  ],
  "graphql-schema-linter": {
    "rules": [
      "defined-types-are-used",
      "deprecations-have-a-reason",
      "descriptions-are-capitalized",
      "enum-values-all-caps",
      "fields-are-camel-cased",
      "input-object-values-are-camel-cased",
      "relay-connection-types-spec",
      "relay-connection-arguments-spec",
      "types-are-capitalized"
    ]
  }
}
